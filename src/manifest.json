{
        "manifest_version": 2,
        "name": "I before E",
        "version": "1.1",
        "description": "Ensures that I is always before E, except after C.",
        "icons": { "128": "128.png",
                    "48": "48.png",
                    "32": "32.png",
                    "16": "16.png" },
        "content_scripts":
        [
                {
                        "matches": ["*://*/*"],
                        "js": ["main.js"],
                        "run_at": "document_end"
                }
        ]
}
